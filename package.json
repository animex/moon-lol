{
  "name": "fsm-demo-rust",
  "scripts": {
    "extract": "cargo run --example extract",
    "example:classic_fiora": "cargo run --package moon_lol --example classic_fiora",
    "example:attack": "cargo run --package moon_lol --example attack",
    "wsl:example:classic_fiora": "WGPU_ALLOW_UNDERLYING_NONCOMPLIANT_ADAPTER=1 cargo run --package moon_lol --example classic_fiora",
    "wsl:example:navigation_grid": "WGPU_ALLOW_UNDERLYING_NONCOMPLIANT_ADAPTER=1 cargo run --package moon_lol --example navigation_grid",
    "wsl:example:attack": "WGPU_ALLOW_UNDERLYING_NONCOMPLIANT_ADAPTER=1 cargo run --package moon_lol --example attack",
    "wsl:example:attack:watch": "WGPU_ALLOW_UNDERLYING_NONCOMPLIANT_ADAPTER=1 cargo-watch -x \"run --example attack\" -w src -w assets",
    "wsl:example:particle:watch": "WGPU_ALLOW_UNDERLYING_NONCOMPLIANT_ADAPTER=1 cargo-watch -w src -w assets/shaders -x \"run --example particle\""
  },
  "dependencies": {
    "@gltf-transform/core": "^4.2.0",
    "@gltf-transform/extensions": "^4.2.0",
    "@types/node": "^24.0.14",
    "three": "^0.178.0",
    "vite": "^6.0.6"
  },
  "devDependencies": {
    "@types/three": "^0.178.1"
  }
}
